	Journal = {Comp. Mat. Sci.},
	Keywords = {high pressures; finite systems; volume; molecular dynamics},
	Pages = {526-536},
	Publisher = {ELSEVIER SCIENCE BV},
	Timescited = {3},
	Title = {Assessment of isobaric-isothermal (NPT) simulations for finite systems},
	Volume = {37},
	Year = {2006},
