	Issn = {0021-9606},
	Journal = {J. Chem. Phys.},
	Month = {Nov 15},
	Number = 19,
	Pages = {10209-10223},
	Title = {Effects of longitudinal quadrupoles on the phase behavior of a Gay-Berne fluid},
	Uri = {<Go to ISI>://000186273200027},
	Volume = 119,
	Year = 2003}

