	Journal = {Biochem. J.},
	Number = {2},
	Pages = {719-730},
	Title = {{Calculation of Hydrodynamic Properties of Globular Proteins from Their Atomic-Level Structure}},
	Url = {http://www.biophysj.org/cgi/content/abstract/78/2/719},
	Volume = {78},
